%======================================================%
% Multiplicities (fully intuitionistic systems)
%======================================================%

LF mult : type =
| ω : mult; % unrestricted

% α₁ • α₂ = α

LF • : mult → mult → mult → type =
| •/ω : • ω ω ω;

% α is unrestricted

LF unr : mult → type =
| unr/ω : unr ω;

% α is an identity element w.r.t. •

LF ident : mult → type =
| ident/ω : ident ω;

% α = α

LF mult_eq : mult → mult → type =
| mult/refl : mult_eq α α;
