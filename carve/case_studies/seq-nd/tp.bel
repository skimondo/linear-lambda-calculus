%======================================================%
% Types
%======================================================%

LF tp : type =
| ① : tp
| ⊗ : tp → tp → tp
| & : tp → tp → tp
| -o : tp → tp → tp
| ⊕ : tp → tp → tp;
--infix ⊗ 5 left.
--infix ⊕ 4 left.
--infix & 4 left.
--infix -o 5 right.

% Type equality

LF eq : tp → tp → type = refl : eq A A;